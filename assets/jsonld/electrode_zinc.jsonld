{
    "@context": "https://w3id.org/emmo/domain/electrochemistry/context",
    "@type": "Electrode",
    "rdfs:label": "Demo Zinc Electrode",
    "rdfs:comment": ["This example shows how to describe an electrode using the 'hasActiveMaterial' property.", "Using 'hasActiveMaterial' with 'Zinc' is equivalent to using the 'ZincElectrode' term in the ontology.", "Electrode types that are needed to describe IEC regulated electrochemical cell definitions are included in the ontology (e.g. 'ZincElectrode'). Use 'hasActiveMaterial' as a more flexible alternative to define other electrode types that are outside the scope of specific regulations."],
    "hasActiveMaterial": {
       "@type": "Zinc"
    },
    "hasProperty": [
       {
             "@type": "SpecificCapacity",
             "hasNumericalPart": {
                "@type": "Real",
                "hasNumericalValue": 800
             },
             "hasMeasurementUnit": "emmo:MilliAmpereHourPerGram"
       },
       {
             "@type": "Thickness",
             "hasNumericalPart": {
                "@type": "Real",
                "hasNumericalValue": 250
             },
             "hasMeasurementUnit": "emmo:MicroMetre"
       },
       {
             "@type": "Diameter",
             "hasNumericalPart": {
                "@type": "Real",
                "hasNumericalValue": 2
             },
             "hasMeasurementUnit": "emmo:CentiMetre"
       },
       {
             "@type": "Mass",
             "hasNumericalPart": {
                "@type": "Real",
                "hasNumericalValue": 0.56
             },
             "hasMeasurementUnit": "emmo:Gram"
       }
    ]
 }